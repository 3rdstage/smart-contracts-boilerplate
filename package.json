{
  "name": "smart-contracts",
  "description": "...",
  "keywords": [
    "Smart Contract",
    "Ethereum",
    "Solidity"
  ],
  "version": "0.0.1",
  "author": {
    "name": "...",
    "email": "...",
    "url": ".."
  },
  "contributors": [],
  "bundleDependencies": false,
  "deprecated": false,
  "files": [
    "/contracts/**/*.sol",
    "/build/truffle/*.json",
    "!/mocks",
    "!/examples"
  ],
  "dependencies": {
    "@openzeppelin/contracts-3": "npm:@openzeppelin/contracts@3.4.2",
    "@openzeppelin/contracts-4": "npm:@openzeppelin/contracts@4.5.0",
    "@truffle/hdwallet-provider": "~1.7.0"
  },
  "devDependencies": {
    "@openzeppelin/test-environment": "~0.1.9",
    "@openzeppelin/test-helpers": "~0.5.13",
    "@remix-project/remixd": "~0.6.9",
    "@typechain/truffle-v5": "~8.0.2",
    "@typechain/web3-v1": "^6.0.0",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.3",
    "@types/chance": "^1.1.3",
    "@types/mocha": "^9.1.1",
    "bip39": "~3.0.4",
    "chance": "^1.1.8",
    "elliptic": "6.5.4",
    "eth-lib": "0.2.8",
    "rlp": "~2.2.6",
    "secp256k1": "4.0.2",
    "truffle-contract-size": "^2.0.1",
    "typechain": "~8.1.1",
    "typedoc": "~0.23.24"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16.0",
    "truffle": ">=5.5.0",
    "typescript": ">=4.5.0",
    "ts-node": ">=10.9.1",
    "@types/node": ">=18.0.0"
  },
  "scripts": {
    "//": [
      "Recommanded Truffle version is 5.3.14 and not more.",
      "https://docs.npmjs.com/files/package.json",
      "https://github.com/OpenZeppelin/zeppelin-solidity#getting-started"
    ],
    "preinstall": "",
    "compile-contracts": "truffle compile contracts/**/*.sol",
    "contract-size": "truffle run contract-size --contracts ...",
    "generate-types": "typechain --always-generate-overloads --target=truffle-v5 'build/contracts/*.json'",
    "generate-typedoc": "npx typedoc",
    "remove-remix-artifacts": "find contracts -type d -name artifacts -exec rm -Rf {} +",
    "remixd": "./scripts/remixd-start.sh",
    "ganache": "./scripts/ganache-cli-start.sh",
    "ganache-refreshed": "./scripts/ganache-cli-start.sh -r",
    "ganache-log": "tail -n 100 ./run/ganache/ganache.log",
    "print-test-keypairs": "node ./scripts/print-test-keypairs.js",
    "slither": "find -E contracts -regex '.*\\.sol' -exec slither {} \\;",
    "solhint": "solhint -f table contracts/**/*.sol",
    "mythril": "find -E contracts -regex '.*\\.sol' -and -not -regex '.*(I|Abstract)[^/]*\\.sol' -exec myth -v 4 analyze --execution-timeout 120 {} \\;"
  }
}
